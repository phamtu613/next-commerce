  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "prisma:generate": "pnpm prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "pnpm prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:push": "pnpm prisma db push --schema=./prisma/schema.prisma",
    "prisma:studio": "pnpm prisma studio --schema=./prisma/schema.prisma",
    "prisma:pull": "pnpm prisma db pull --schema=./prisma/schema.prisma",
    "prisma:deloy": "pnpm prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:status": "pnpm prisma migrate status --schema=./prisma/schema.prisma",
    "prisma:migrate:squash:ecom": "dotenv -e .env -- bash ./prisma/scripts/squash-migrations.sh --schema=./prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
